#include<stdio.h>
int main()
{int a,b,i,power=1,t;
printf("Enter the value of a and b : ");
scanf("%d%d",&a,&b);
t=b;
if(b<0)
{b=-b;
}
else
{b=b;
}
for(i=1;i<=b;i++)
{power=power*a;
}
if(t<0)
{printf("%d to the power %d is : %d",a,b,1/power);
}
else
printf("%d to the power %d is : %d",a,b,power);
return 0;
}
